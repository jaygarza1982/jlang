include:print.jlang

func main() {
	global byte hello_new_line = "Hello, World!",10
	print(hello_new_line, jlang.global_len(hello_new_line))

	global byte jlang_message = "I think JLang is pretty neat!",10,10
	print(jlang_message, jlang.global_len(jlang_message))

	global byte final_message = "This line marks the end of the program.",10
	print(final_message, jlang.global_len(final_message))
}
